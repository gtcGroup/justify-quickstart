<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="off">
  <Appenders>
    <Console name="CONSOLE" target="SYSTEM_OUT">
      <PatternLayout pattern="[%p] %d [%t] %c - %m%n"/>
    </Console>
    <Async name="ASYNC">
      <AppenderRef ref="CONSOLE"/>
    </Async>
  </Appenders>
  <Loggers>
    <Root level="INFO">
      <AppenderRef ref="ASYNC"/>
    </Root>
  </Loggers>
</Configuration>